{
  "name": "van-view-template",
  "private": true,
  "version": "0.0.3",
  "type": "module",
  "scripts": {
    "clean_node_modules": "rimraf node_modules",
    "reinstall": " rimraf pnpm-lock.yaml && rimraf package.lock.json && rimraf node_modules && npm run bootstrap",
    "bootstrap": "pnpm install",
    "clear:cache": "pnpm cache clean --force",
    "commit": "git-cz",
    "dev": "vite",
    "build:pro": "cross-env NODE_ENV=production vue-tsc --noEmit&& vite build",
    "build:test": "cross-env  vue-tsc --noEmit&& vite build --mode test ",
    "dist:clean": "esno ./build/script/cleanDist.ts",
    "preview": "vite preview",
    "lint:eslint": "eslint --fix --ext .js,.ts,.vue ./src",
    "lint:format": "prettier --write --loglevel warn \"src/**/*.{js,ts,json,tsx,css,less,vue,html,md}\"",
    "lint:style": "stylelint --fix \"**/*.{vue,less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "prepare": "husky install",
    "ts:check": "vue-tsc --noEmit",
    "lint:lint-staged": "lint-staged -c ./.husky/lintstagedrc.cjs",
    "release:alpha": "standard-version --prerelease alpha",
    "release:patch": "standard-version --release-as patch",
    "release:minor": "standard-version --release-as minor",
    "release:major": "standard-version --release-as major",
    "analysis": "windicss-analysis",
    "preinstall": "npx only-allow pnpm",
    "release": "release-it"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/badlym/van-view-template"
  },
  "dependencies": {
    "@vitejs/plugin-legacy": "^4.0.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vueuse/core": "^10.1.0",
    "autoprefixer": "^10.4.14",
    "axios": "^1.3.6",
    "bignumber.js": "^9.1.1",
    "cnjm-postcss-px-to-viewport": "^1.0.0",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.7",
    "dotenv": "^16.0.3",
    "fs-extra": "^11.1.1",
    "js-md5": "^0.7.3",
    "less": "^4.1.3",
    "lodash-es": "^4.17.21",
    "mockjs": "^1.1.0",
    "node-html-parser": "^6.1.5",
    "picocolors": "^1.0.0",
    "pinia": "^2.0.35",
    "qs": "^6.11.1",
    "release-it": "^15.10.3",
    "resize-observer-polyfill": "^1.5.1",
    "rimraf": "^5.0.0",
    "vant": "^4.2.0",
    "vconsole": "^3.15.0",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-vconsole": "^1.3.1",
    "vue": "^3.2.47",
    "vue-eslint-parser": "^9.1.1",
    "vue-types": "^5.0.2",
    "web-storage-cache": "^1.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^17.6.1",
    "@minko-fe/postcss-pxtorem": "^1.2.1",
    "@originjs/vite-plugin-commonjs": "^1.0.3",
    "@types/js-md5": "^0.7.0",
    "@types/lodash-es": "^4.17.7",
    "@types/node": "^18.16.1",
    "@types/qs": "^6.9.7",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "@vitejs/plugin-vue": "^4.2.1",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-define-config": "^1.19.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.11.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.1",
    "postcss": "^8.4.23",
    "postcss-html": "^1.5.0",
    "postcss-less": "^6.0.0",
    "postcss-load-config": "^4.0.1",
    "postcss-px-to-viewport-8-plugin": "^1.2.2",
    "prettier": "^2.8.8",
    "rollup-plugin-compression": "^1.0.2",
    "sass": "^1.62.1",
    "standard-version": "^9.5.0",
    "stylelint": "^15.6.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recommended-less": "^1.0.4",
    "stylelint-config-standard": "^33.0.0",
    "stylelint-config-standard-vue": "^1.0.0",
    "stylelint-less": "^1.0.6",
    "stylelint-order": "^6.0.3",
    "terser": "^5.17.1",
    "typescript": "^5.0.4",
    "unplugin-auto-import": "^0.15.3",
    "unplugin-vue-components": "^0.24.1",
    "unplugin-vue-define-options": "^1.3.3",
    "vite": "^4.3.2",
    "vite-plugin-mock": "^2.9.6",
    "vite-plugin-vue-setup-extend": "^0.4.0",
    "vite-plugin-windicss": "^1.8.10",
    "vite-plugin-zip-file": "^2.0.0",
    "vue-router": "^4.1.6",
    "vue-tsc": "^1.4.4",
    "windicss": "^3.5.6",
    "windicss-analysis": "^0.3.5"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}
